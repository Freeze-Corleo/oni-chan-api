{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "test": "jest",
    "heroku-prebuild": "npm i -g clinic ",
    "dev": "npx nodemon --ignore swagger_output_file.json src/index.ts",
    "predev": "npx prisma generate && npm run migrate",
    "migrate": "npx prisma db push",
    "start": "node build/src/index.js",
    "pretty-quick": "pretty-quick",
    "lint": "eslint src/**/*.ts",
    "build": "npx tsc --build --verbose && cp ./swagger_output_file.json ./build",
    "prepare": "husky install .husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx pretty-quick --staged ng lint ng test",
      "pre-push": "ng build --aot true"
    }
  },
